= form_for(@project) do |f|
  = f.label(:name)
  = f.text_field(:name)
  - if @project.owner.present?
    %p Owned By
    =image_tag "#{@owner.profile_image_url}"
    %span.display-name= @owner.display_name
  = f.label(:office)
  = f.select(:office, ['SF', 'NY', 'other'])
  = f.label(:other_technologies)
  = f.text_field(:other_technologies)
  = f.label(:description)
  = f.text_area(:description)
  = f.submit

