= form_for(@project) do |f|
  = f.label(:name)
  = f.text_field(:name)
  - if @project.owner.present?
    %p Owned By
    %img{src: "#{@project.owner.profile_image_url}"}
    .display-name= @project.owner.display_name
  = f.label(:office)
  = f.select(:office, ['SF', 'NY', 'other'])
  = f.label(:technology)
  = f.text_field(:technology)
  = f.label(:description)
  = f.text_area(:description)
  = f.submit

